{
  "name": "@hp-stuff/hono",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development concurrently -k -n SERVER,CLIENT,CSS,HTML -c green,blue,magenta,cyan \"pnpm:watch:server\" \"pnpm:watch:client\" \"pnpm:watch:css\" \"pnpm:watch:html\"",
    "watch:server": "nodemon --watch src/shared --watch src/server --ext ts,json --exec \"tsx src/server/server.ts\"",
    "watch:client": "chokidar 'src/shared/**/*.ts' 'src/client/**/*.ts' -c 'pnpm build:client' --initial",
    "watch:css": "chokidar 'src/styles/**/*.css' -c 'pnpm build:css' --initial",
    "watch:html": "chokidar 'src/**/*.html' -c 'pnpm build:html' --initial",
    "build:css": "./bin/build-css.sh",
    "build:ts": "tsc && tsc-alias",
    "build:client": "tsx ./src/scripts/build-client.ts",
    "build:html": "./bin/build-Html.sh",
    "build": "./bin/build-server.sh"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.40.0",
    "@hono/node-server": "^1.14.1",
    "@hono/zod-validator": "^0.4.3",
    "@spectrum-css/actionbutton": "^7.1.3",
    "@spectrum-css/alertbanner": "^3.1.1",
    "@spectrum-css/button": "^14.1.6",
    "@spectrum-css/card": "^11.1.0",
    "@spectrum-css/closebutton": "^6.1.0",
    "@spectrum-css/divider": "^5.1.0",
    "@spectrum-css/icon": "^9.1.0",
    "@spectrum-css/link": "^7.1.0",
    "@spectrum-css/menu": "^9.2.0",
    "@spectrum-css/page": "^9.1.0",
    "@spectrum-css/picker": "^9.1.4",
    "@spectrum-css/sidenav": "^7.1.0",
    "@spectrum-css/splitview": "^7.1.0",
    "@spectrum-css/table": "^8.1.0",
    "@spectrum-css/tokens": "^16.0.1",
    "@spectrum-css/typography": "^8.1.0",
    "@spectrum-web-components/card": "^1.5.0",
    "@spectrum-web-components/popover": "^1.5.0",
    "@spectrum-web-components/split-view": "^1.5.0",
    "@spectrum-web-components/styles": "^1.5.0",
    "@spectrum-web-components/theme": "^1.5.0",
    "aws-lambda": "^1.0.7",
    "cosmiconfig": "^9.0.0",
    "cosmiconfig-typescript-loader": "^6.1.0",
    "dotenv": "^16.5.0",
    "gray-matter": "^4.0.3",
    "hastscript": "^9.0.1",
    "hono": "^4.7.6",
    "iconify-icon": "^2.3.0",
    "isomorphic-git": "^1.30.1",
    "lit": "^3.3.0",
    "rehype": "^13.0.2",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-frontmatter": "^5.0.0",
    "signal-utils": "^0.21.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/hast": "^3.0.4",
    "@types/livereload": "^0.9.5",
    "@types/node": "^22.14.1",
    "autoprefixer": "^10.4.21",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "cssnano": "^7.0.6",
