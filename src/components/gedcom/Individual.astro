---
import { Card } from '@astrojs/starlight/components';
import { GedcomIndividual } from './individual';
import { file } from './config';
const GedUrl = new URL('/' + file, Astro.url );

let { id } = Astro.props;
id = `@${id}@`;

console.log(`Gedcom url is ${GedUrl}`)
---

<Card title="Biographical Information">
    <GedcomIndividual gedid={id} url={GedUrl} client:only />
</Card>
